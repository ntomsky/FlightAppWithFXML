<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@Login.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI_Control.Controller">
    <children>
        <VBox layoutX="194.0" layoutY="68.0" prefHeight="264.0" prefWidth="212.0">
         <children>
            <Button fx:id="MyFlightsBtn" mnemonicParsing="false" onAction="#toMyFlightScene" prefHeight="66.0" prefWidth="233.0" text="My  Flights">
               <font>
                  <Font name="Adobe Gurmukhi" size="18.0" />
               </font>
            </Button>
            <Button fx:id="bookNewFlightBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#transitionToBookNewFlight" onKeyPressed="#transitionToBookNewFlight" prefHeight="66.0" prefWidth="233.0" text="Book New Flight">
               <font>
                  <Font name="Adobe Gurmukhi" size="18.0" />
               </font>
            </Button>
            <Button fx:id="backToLogInBtn" layoutX="10.0" layoutY="76.0" mnemonicParsing="false" onAction="#backToLogIn" prefHeight="66.0" prefWidth="233.0" text="Log Out">
               <font>
                  <Font name="Adobe Gurmukhi" size="18.0" />
               </font>
            </Button>
            <Button layoutX="10.0" layoutY="142.0" mnemonicParsing="false" prefHeight="66.0" prefWidth="233.0" text="Delete My Account">
               <font>
                  <Font name="Adobe Gurmukhi" size="18.0" />
               </font>
            </Button>
         </children>
        </VBox>
    </children>
</AnchorPane>
