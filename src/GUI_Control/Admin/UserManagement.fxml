<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI_Control.Admin.UserManagementController">
   <children>
      <TableView fx:id="customerTableView" layoutX="157.0" layoutY="74.0" prefHeight="206.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="customerID_Col" prefWidth="120.0" text="Custoner_ID" />
          <TableColumn fx:id="usernameCol" prefWidth="120.0" text="Username" />
            <TableColumn fx:id="passwordCol" prefWidth="120.0" text="UserPassword" />
            <TableColumn fx:id="firstNameCol" prefWidth="120.0" text="First Name" />
            <TableColumn fx:id="lastNameCol" prefWidth="120.0" text="Last Name" />
        </columns>
      </TableView>
      <Button fx:id="backToAdminBtn" layoutX="30.0" layoutY="550.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Back" />
      <VBox layoutX="376.0" layoutY="331.0" prefHeight="200.0" prefWidth="130.0">
         <children>
            <Button fx:id="deleteCustomerBtn" alignment="CENTER" mnemonicParsing="false" onAction="#processDeleteCustomer" prefHeight="40.0" prefWidth="139.0" text="Delete User" />
            <Button fx:id="addCustomerBtn" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="40.0" prefWidth="142.0" text="Add New User">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
